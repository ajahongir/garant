<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(target_floor) -%>">
  <span class='title bold'>
    <%= target_floor.target.name %>
    <span class="preview">&nbsp;&nbsp;&nbsp;&nbsp;</span>
  </span>

  <span class='title'>
    <%= target_floor.floor %>
    <span class="preview">&nbsp;</span>
  </span>

  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), target_floor_path(target_floor),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_target_floor_path(target_floor),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_target_floor_path(target_floor),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => target_floor.data),
        :method => :delete %>
  </span>
</li>

